select (count(distinct ?s) as ?count)
where {
  graph ?graph1 {
  ?s ?p1 ?o1 .
  }
  graph ?graph2 {
  ?s ?p2 ?o2 .
  }
  graph ?templateGraph1 {
    ?s <http://dbpedia.org/property/wikiPageUsesTemplate> ?t1 .
  }
  graph ?templateGraph2 {
   ?s <http://dbpedia.org/property/wikiPageUsesTemplate> ?t2 .
  }
  FILTER (?o1 != ?o2)
}